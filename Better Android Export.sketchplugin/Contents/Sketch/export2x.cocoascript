@import 'library/common.cocoascript';

var factors = [
  {name: "drawable-mdpi", scale: 0.50},
  {name: "drawable-hdpi", scale: 0.750},
  {name: "drawable-xhdpi", scale: 1.0},
  {name: "drawable-xxhdpi", scale: 1.50},
  {name: "drawable-xxxhdpi", scale: 2.0}];

function onRun(context) {
